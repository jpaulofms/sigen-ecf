package com.sigen.ecf.model.bean;

/**
 *
 * @author Administrador
 */
public class BeanParametros {

    private boolean permiteUtilizacaoCreditoParcial;
    
    private String folderIn;
    
    private String folderOut;

    public boolean isPermiteUtilizacaoCreditoParcial() {
        return permiteUtilizacaoCreditoParcial;
    }

    public void setPermiteUtilizacaoCreditoParcial(boolean permiteUtilizacaoCreditoParcial) {
        this.permiteUtilizacaoCreditoParcial = permiteUtilizacaoCreditoParcial;
    }
    
    public void setFolderIn(String folderIn){
        this.folderIn = folderIn;
    }
    
    public String getFolderIn(){
        return folderIn;
    }
    
    public void setFolderOut(String folderOut){
        this.folderOut = folderOut;
    }
    
    public String getFolderOut(){
        return folderOut;
    }
}
